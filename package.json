{
  "name": "sngrco-astro-template",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "prebuild": "pnpm dlx npm-run-all -s check:*",
    "build": "astro build",
    "check:lint": "astro check && tsc --noEmit",
    "check:style": "pnpm exec prettier --check --plugin-search-dir=. .",
    "dev": "astro dev",
    "format": "pnpm exec prettier --write --plugin-search-dir=. .",
    "preview": "astro preview",
    "start": "pnpm dlx npm-run-all -s build preview"
  },
  "dependencies": {
    "astro": "^2.1.9",
    "typescript": "^5.0.3"
  }
}
