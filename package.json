    "astro": "1.9.2",
    "astro-compress": "1.1.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "unplugin-icons": "0.15.3"
  },
  "devDependencies": {
    "@svgr/core": "6.5.1",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "autoprefixer": "10.4.14",
    "daisyui": "2.51.5",
    "postcss": "8.4.21",
    "sass": "1.60.0",
    "tailwindcss": "3.2.7",
