{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "astro": "astro",
    "prebuild": "pnpx npm-run-all -s check",
    "build": "astro check && tsc --noEmit && astro build",
    "check": "pnpx prettier --check --plugin-search-dir=. .",
    "dev": "astro dev",
    "format": "pnpx prettier --write --plugin-search-dir=. .",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "start": "pnpx npm-run-all -s build preview"
  },
  "dependencies": {
    "@astrojs/react": "1.2.2",
    "@fontsource/unbounded": "^4.5.0",
    "@iconify/json": "2.1.153",
    "@vite-pwa/astro": "^0.0.1",
    "astro": "1.7.1",
    "astro-compress": "^1.1.22",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "unplugin-icons": "0.14.15"
  },
  "devDependencies": {
    "@svgr/core": "6.5.1",
    "@tailwindcss/typography": "0.5.8",
    "@types/node": "18.11.16",
    "@types/react": "18.0.26",
    "autoprefixer": "10.4.13",
    "daisyui": "2.45.0",
    "postcss": "8.4.20",
    "sass": "1.57.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "vite-plugin-pwa": "^0.14.0",
    "workbox-window": "^6.5.4"
  }
}
